NODE_ENV=development
PORT=3000

# MongoDB connection string
# e.g. mongodb://127.0.0.1:27017 or Atlas SRV string
MONGODB_URI=

# Database and collection
MONGODB_DB_NAME=vector_db
MONGODB_COLLECTION=documents

# OpenAI API key and embedding config
OPENAI_API_KEY=
EMBEDDING_MODEL=text-embedding-3-small
# 1536 for text-embedding-3-small, 3072 for text-embedding-3-large
EMBEDDING_DIM=1536

# Set to true if using MongoDB Atlas Vector Search with an index
USE_ATLAS_VECTOR=false


